{
  "name": "react-webpack-boilerplate",
  "version": "2.0.0",
  "description": "React webpack boilerplate.",
  "main": "index.js",
  "scripts": {
    "release": "npm run clean && node ./scripts/release.js",
    "build": "./node_modules/.bin/webpack",
    "clean": "rm -rf dist coverage",
    "coveralls": "npm run test && cat ./coverage/lcov/lcov.info | coveralls",
    "lint": "node_modules/.bin/eslint ./src",
    "start": "node server.js",
    "test": "npm run lint && node_modules/.bin/karma start --single-run --no-auto-watch karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeantimex/react-webpack-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "mocha",
    "chai",
    "sinon",
    "isparta",
    "scss",
    "es6",
    "babel",
    "webpack",
    "karma"
  ],
  "author": "Yong Su (https://github.com/jeantimex)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeantimex/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/jeantimex/react-webpack-boilerplate",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.3"
  },
  "devDependencies": {
    "async": "^2.1.4",
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-intl-to-properties": "^2.0.3",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "core-js": "^2.4.1",
    "coveralls": "^2.11.16",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "glob": "^7.1.1",
    "intl": "^1.2.5",
    "intl-messageformat-parser": "^1.2.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "karma": "^1.4.1",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
